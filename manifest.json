{
  "name": "C&P Chrome Extension",
  "version": "1.0",
	"manifest_version": 2,  
	"description": "Bugs ;)",
  "browser_action": {
    "default_icon": "icon.logo.gif"
  },
  "options_page": "settings.html",
	"permissions": [
		"https://*/*",
		"http://*/*",
		"tabs"
	],
	"content_scripts": [
	  {
	    "matches": ["http://cnpx-web.hzdesign.com/*"],
	    "js": [
	      "jquery.min.js",
	      "jquery-ui/js/jquery-ui-1.8.18.custom.min.js",
	      "lib/underscore-min.js",
	      "lib/backbone-min.js",
	      "app/models.js",
	      "app/controller.js"
	    ]
	  }
	]
}
