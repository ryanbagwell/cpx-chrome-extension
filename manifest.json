{
  "name": "Clients and Profits Time Card Autocomplete",
  "version": "1.0",
	"manifest_version": 2,
	"description": "Adds autocomplete functionality to the Clients & Profits web time card",
  "icons": {
    "16": "images/icon.logo.16.png",
    "48": "images/icon.logo.48.png",
    "128": "images/icon.logo.128.png"
  },
  "browser_action": {
    "default_icon": {
        "19": "images/icon.logo.19.png",
        "38": "images/icon.logo.38.png"

    },
    "default_title": "Clients & Profits",
    "name": "Clients & Profits"
  },
  "options_page": "settings.html",
  "permissions": [
    "https://*/*",
	"http://*/*",
	"tabs",
    "activeTab"
  ],
  "background": {
    "scripts":[
      "lib/underscore-min.js",
      "app/browseraction.js"
    ]
  }
}
